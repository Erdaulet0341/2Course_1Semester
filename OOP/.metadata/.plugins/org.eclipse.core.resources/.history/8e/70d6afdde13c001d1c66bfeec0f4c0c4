package Problem5;
import java.util.Scanner;

public class testDragon {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		boolean checkEnd= true;
		while(checkEnd) {
			String name = in.next();
			if (name.equals("End!")) {
				checkEnd = false;
			}
			else{
				Person p = new Person(name);
			}
		}
		for(Person i: Person.person) {
			System.out.print(i);
		}
	}

}
